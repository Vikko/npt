Heart rate monitor
<br/>
<div id="wrapper">
	<div id="live_heart_rate_plot"></div>
	<div id="bpm_rate_plot" style=""></div>
	<div id="bpm_value" style="">
		<div class="header">
			<strong>Current heartbeat</strong>
		</div>
		<div class="bpm-value">

		</div>
	</div>
	<div class="clearfix"/>
</div>

<p>
<!-- <a id="start" href="#">Start</a>
<a id="stop" href="#;">Stop</a> -->
POSTS log:<br/>
<% @logger.each do |log_entry| %>
	<%= log_entry.created_at %> || <%= log_entry.content %> <br/>
<% end %>
<br/>
<a id="test_post" href="#">Test post action</a> <%= @post_count %>
</p>

<script type="text/javascript">
	$(document).ready(function($){
		debug = "";
		$("#test_post").click(function(e){
			$.ajax({
				url: "<%= url_for(:action => :post_data) %>",
				type: 'POST',
				dataType: 'json',
			  success: function(data){
				debug = data
			    alert('success! \nstatus: ' + data.status);
			  },
			  error: function(data){
			    alert('failure: ' + data);
			  }
			});
		});
	})
</script>